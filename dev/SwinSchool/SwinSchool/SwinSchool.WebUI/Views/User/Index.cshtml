@model IEnumerable<SwinSchool.CommonShared.Dto.MyUserDto>

@{
    ViewBag.Title = "Index";
}

<h2>User list</h2>

<p>
    @Html.ActionLink("Create New User", "Create") | 
    @Html.ActionLink("Cart", "Index", "Cart")
</p>
<table class="table table-responsive">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecQn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecAns)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecQn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecAns)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.UserID })
        </td>
    </tr>
}

</table>
